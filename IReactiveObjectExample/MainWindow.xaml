<Window x:Class="IReactiveObjectExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IReactiveObjectExample"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <TabControl>
        <TabItem Header="ReactiveObject Filters">
            <local:MyUserControl x:Name="ReactiveObject" />
        </TabItem>
        <TabItem Header="IReactiveObject Filters">
            <local:MyUserControl x:Name="IReactiveObject" />
        </TabItem>
    </TabControl>
</Window>
